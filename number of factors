import java.util.*;
class factor
{
public static void main(String args[])
{
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int f=0;
int[] a=new int[n];
for(int i=1;i<=n;i++){
if(n%i==0)
{
a[f]=i;
f=f+1;
}
}
System.out.println("no of factors: "+f);
System.out.println(" enter the value:");
int nth=s.nextInt();
System.out.println(+a[n-1]);
}
}
