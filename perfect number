import java.util.*;
class perfect
{
public static void main(String args[])
{
Scanner s= new Scanner(System.in);
System.out.println("Enter the number: ");
int x =s.nextInt();
int sum=0;
for(int i=1; i<x; i++)
{
if(x%i==0)
{
sum=sum+2;
}}
if(x==sum)
{
System.out.println("perfect number");
}
else
{
System.out.println("not perfect number");
}
}
}
